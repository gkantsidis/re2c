    <ReToCInputs Include="..\..\..\re2c\src\conf\parse_opts.re" />
  </ItemGroup>
  <Target Name="CreateReToCDirectoryTask">
    <MakeDir Directories="$(GeneratedDirectory)" />
  </Target>
  <Target Name="CompileReToCTask" DependsOnTargets="CreateReToCDirectoryTask" Inputs="@(ReToCInputs)" Outputs="@(ReToCInputs->'$(GeneratedDirectory)%(Filename).cc')">
    <Exec Command="'$(OutDir)\re2c.bootstrap.exe' '%(ReToCInputs.Identity)' -o @(ReToCInputs->'$(GeneratedDirectory)%(Filename).cc')" />
  </Target>
  <Target Name="Bison" Inputs="$(ProjectDir)..\..\..\re2c\src\ast\parser.ypp" Outputs="$(GeneratedDirectory)parser.cc;$(GeneratedDirectory)y.tab.h">
    <Exec Command="'$(ProjectDir)..\..\packages\bison-win32\tools\native\bin\bison.exe' --output='$(GeneratedDirectory)parser.cc' --defines='$(GeneratedDirectory)y.tab.h' '$(ProjectDir)..\..\..\re2c\src\ast\parser.ypp'" />
  </Target>
  <Target Name="BeforeBuild" DependsOnTargets="CompileReToCTask;Bison">
    <Message Text="Running re2c.bootstrap" />
  </Target>
  <PropertyGroup>
    <BuildDependsOn>
	  CreateReToCDirectoryTask;
	  Bison;
	  CompileReToCTask;
      $(BuildDependsOn)
    </BuildDependsOn>
  </PropertyGroup>
  <ItemGroup>
    <ClCompile Include="@(ReToCInputs->'$(GeneratedDirectory)%(Filename).cc')" />
    <ClCompile Include="$(GeneratedDirectory)parser.cc" />
